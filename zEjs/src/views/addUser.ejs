<h1>Bienvenido!!</h1>
<form id="usuarioForm">
    <label>Ingrese Nombre</label>
    <input name="nombre" required>
    <label>Ingrese Apellido</label>
    <input name="apellido" required>
    <input type="submit">
</form>
<input type="button" onclick="location.href='/usuarios'" value="Listado">

<script>
    let usuarioForm = document.getElementById('usuarioForm');
    let result
    console.log("conected")

    const handleSubmit = (evt, form, route) => {
        // evt.preventDefault();
        let formData = new FormData(form);
        let obj = {};
        formData.forEach((value, key) => obj[key] = value);
        fetch(route, {
            method: "POST",
            body: JSON.stringify(obj),
            headers: {
                "Content-Type": "application/json"
            }
        })

    }
    usuarioForm.addEventListener('submit', (e) => handleSubmit(e, e.target, '/api/usuarios'));
</script>
<link rel="stylesheet" href="styles.css">